{
    "preset": "laravel",
    "rules": {
        "braces": false,
        "concat_space": {
            "spacing": "one"
        },
        "curly_braces_position": false,
        "new_with_braces": {
            "anonymous_class": false,
            "named_class": false
        },
        "not_operator_with_successor_space": false,
        "simplified_null_return": true
    }
}
